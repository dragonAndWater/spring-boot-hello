<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.executer.bookCharge.dao.BookChargeDao">

    <resultMap id="BaseResultMap" type="com.example.demo.executer.bookCharge.model.BookChargeModel">
        <result column="id" property="id"/>
        <result column="book_type" property="bookType"/>
        <result column="fee_amt" property="feeAmt"/>
        <result column="fee_type" property="feeType"/>
        <result column="update_time" property="updateTime"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <insert id="insertOne" parameterType="com.example.demo.executer.bookCharge.model.BookChargeModel">
        insert into t_book_charge(
        id,
        book_type,
        fee_amt,
        fee_type,
        )
        values(
        #{id},
        #{bookType},
        #{feeAmt},
        #{feeType}
        );
    </insert>

    <update id="updateOne" parameterType="com.example.demo.executer.bookCharge.model.BookChargeModel">
        update t_book_charge set
        <trim suffixOverrides=",">
            <if test="bookType != null">
                book_type = #{bookType},
            </if>
            <if test="feeAmt != null">
                fee_amt = #{feeAmt},
            </if>
            <if test="feeType != null">
                fee_type = #{feeType},
            </if>
        </trim>
        <where>
            id = #{id}
        </where>
    </update>

    <select id="selectOne" parameterType="java.lang.String" resultMap="BaseResultMap">
        select id,
               book_type,
               fee_amt,
               fee_type,
               create_time,
               update_time
        from t_book_charge
        where id = #{id}
    </select>


</mapper>