  `id` bigint(20) NOT NULL COMMENT 'user_id',
  `group_team` varchar(4) COLLATE utf8_bin DEFAULT NULL COMMENT '所属班组',
  `person_type` varchar(4) COLLATE utf8_bin DEFAULT NULL COMMENT '人员类型',
  `education` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '最高学历',
  `first_degree_major` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '第一学习专业',
  `stagnation_point` varchar(256) COLLATE utf8_bin DEFAULT NULL COMMENT '所属驻点名称',
  `install_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '装维星级认证',
  `base_stat_skil_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '基站技能认证',
  `sub_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '综合代维技能达标认证',
  `tower_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '铁塔认证:无,初级,中级,高级',
  `is_ther_a_clim_perm` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '是否有登高证',
  `is_ther_an_elec_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '是否有电工证',
  `is_limit_sp_opt_cert` varchar(20) COLLATE utf8_bin DEFAULT NULL COMMENT '是否有有限空间作业证',
  `maintP_prof` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '维护工作的类型（可多选）',