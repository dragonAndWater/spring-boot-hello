  `id` bigint(20) NOT NULL COMMENT '建议反馈id',
  `report_type` tinyint(2) NOT NULL COMMENT '涉及类型 1-涉政类，2-涉黄类，3-涉赌类，4-涉诈类，5-涉暴类，6-其他',
  `report_link` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '举报连接',
  `content` varchar(2048) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '举报内容',
  `user_name` varchar(64) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '用户名',
  `user_phone` bigint(11) NOT NULL COMMENT '用户手机号',
  `del_flag` tinyint(1) NOT NULL DEFAULT '0' COMMENT '当前状态（0-未删除，1-已删除）',
  `remark` varchar(512) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '备注',
  `create_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',