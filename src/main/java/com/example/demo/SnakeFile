 `id` bigint(20) NOT NULL COMMENT '建议反馈id',
  `proposal_type` tinyint(2) NOT NULL COMMENT '建议类型 1-优化建议，2-功能缺陷，3-文档，4-其他',
  `title` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '建议标题',
  `proposal_module` tinyint(2) NOT NULL COMMENT '功能模块 1-物联网设计，2-硬件产品接入，3-应用产品接入，4-仿真管理，5-物联网专业服务，6-文档中心',
  `images` varchar(1024) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '截图id 多个截图之间使用逗号分隔（英文逗号,）',
  `state` tinyint(1) NOT NULL DEFAULT '0' COMMENT '处理结果：0-未处理，1-已处理，2-处理中',
  `user_id` bigint(20) NOT NULL COMMENT '用户id',
  `user_name` varchar(64) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '用户名',
  `user_phone` bigint(11) NOT NULL COMMENT '用户手机号',
  `del_flag` tinyint(1) NOT NULL DEFAULT '0' COMMENT '当前状态（0-未删除，1-已删除）',
  `remark` varchar(512) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '备注',
  `create_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',