  `id` bigint(20) NOT NULL COMMENT '地区ID',
  `pid` bigint(20) DEFAULT NULL COMMENT '上级地区ID',
  `p_ids` varchar(500) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '上级地区IDS',
  `p_names` varchar(500) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '上级地区names',
  `areaCode` varchar(12) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '地区编码',
  `areaName` varchar(100) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '地区名',
  `sort` tinyint(4) DEFAULT NULL COMMENT '展示排序',
  `level` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL DEFAULT '-1' COMMENT '地区级别：1:省份province，2:市city，3:区县district，4:街道/乡street，5：社区/村',
  `cityCode` varchar(4) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '城市区号：长途电话区号',
  `center` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '城市中心点：经纬度坐标',
  `is_del` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除标记：0未删除，1删除',
  `create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',